@model RM.Unify.Sdk.SampleApp2.ViewModels.LoginViewModel

@using (Html.BeginForm())
{
  <div class="narrowcontent">
    <h2 style="text-align:center">
      Welcome to School Blog
    </h2>
    <!-- RMUNIFY -->
    <div style="margin:15px 0 30px 0; text-align:center">
      <div class="col-md-12">
        <style type="text/css">a.rmunify { display: inline-block; text-decoration: none; outline: medium none; background: none; margin: 0; padding: 0; width: 179px; height: 45px; background-image: url(data:image/png;base64,); } a.rmunify:hover { background-image: url(data:image/png;base64,); } a.rmunify:active { background-image: url(data:image/png;base64,); }</style>
        <a class="rmunify" href="/RMUnify@( Request.Params["returnurl"] == null ? "" : "?ReturnUrl=" + Request.Params["returnurl"] )" ></a>
      </div>
    </div>
    <div style="margin-bottom: 10px">Or enter your School Blog login and password:</div>
    <!-- END RMUNIFY -->
    @if (ViewBag.ValidationSummary != null)
    {
      @Html.ValidationSummary(true, (string) ViewBag.ValidationSummary, new { Class = "alert alert-danger" })
    }
    else
    {
      @Html.ValidationSummary(true, "Invalid username or password", new { Class = "alert alert-danger" })
    }
    <div class="form-group">
      @Html.TextBoxFor(m => m.LoginName, new { placeholder = "Login name", autofocus = "true", Class = "form-control" })
    </div>
    <div class="form-group">
      @Html.PasswordFor(m => m.Password, new { placeholder = "Password", Class = "form-control" })
    </div>
    <button class="btn btn-primary" type="submit">
      Sign in
    </button>
  </div>
}

